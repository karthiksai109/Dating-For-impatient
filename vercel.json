{
  "version": 2,
  "buildCommand": "cd Dating-For-impatient-Frontend/user-frontend && npm install && npm run build",
  "outputDirectory": "Dating-For-impatient-Frontend/user-frontend/build",
  "installCommand": "npm install && cd Dating-For-impatient-Frontend/user-frontend && npm install",
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.js" },
    { "source": "/((?!api/).*)", "destination": "/index.html" }
  ]
}
